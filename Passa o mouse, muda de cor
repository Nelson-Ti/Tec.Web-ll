 <div> Teste div</div>
 <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
 <script>
 $( document ).ready(function() {
 //Código das funcoes em jquery
 $("p").mouseover(function() { //quando o mouse passar por cima do parágrafo
 $(this).attr("style", "background-color:red; color:white");
 });

 $("p").mouseleave(function() { //quando o mouse sair de cim a do parágrafo
 $(this).attr("style", "background-color:white; color:black");
 });
 });
 </script>
