    <script>


		var html;
		$( document ).ready(function() {
			$.each(objeto, function(i, item) {
			
	html = "<option value='"+item.nome+"'>"+item.nome+"</option>";
				
				if (item.posicionamento=="triagem") {
					$("#triagem").append(html);
				//}else if(item.posicionamento=="geral") {
					//$("#clinico").append(html);
				//}else if(item.posicionamento=="emergencia") {
					//$("#emergencia_caixa").append(html);
				//}else if(item.posicionamento=="alta") {
					//$("#fim").append(html);					
				}
			});//for each







			
			$("#geral").click(function() {
				var selecionado = $("#triagem option:selected").val();
		html = "<option value='"+selecionado+"'>"+selecionado+"</option>";
				$("#clinico").append(html);
				//console.log(selecionado);
			});
			
			$("#emergencia").click(function() {
				var selecionado = $("#triagem option:selected").val();
				html = "<option value='"+selecionado+"'>"+selecionado+"</option>";
				$("#emergencia_caixa").append(html);
			});

			$("#alta").click(function() {
				var selecionado = $("#clinico option:selected").val();
				html = "<option value='"+selecionado+"'>"+selecionado+"</option>";
				$("#fim").append(html);
			});
			

		});//fim document


		

			
			




    </script>
